{
	"Vue init": {
		"prefix": "vinit",
		"body": [
			"<script lang=\"ts\" setup>",
			"",
			"</script>",
			"",
			"<template>",
			"",
			"</template>",
			"",
			"<style scoped>",
			"",
			"</style>"
		],
		"description": "使用组合式 api 创建 vue 文件"
	},
	"Vue Define Props": {
		"prefix": "vprops",
		"body": [
			"const props = defineProps<{",
			"\t${1:key}: ${2:string}",
			"}>()"
		],
		"description": "TS 定义 Props"
	},
	"Vue Define Props with Defaults": {
		"prefix": "vpropsd",
		"body": [
			"const props = withDefaults(defineProps<{",
			"\t${1:key}: ${2:string}",
			"}>(), {",
			"\t$1: ${3:defaultValue}",
			"})"
		],
		"description": "TS 定义带默认值的 Props"
	},
	"Vue Define Emits": {
		"prefix": "vemits",
		"body": [
			"const emit = defineEmits<{",
			"\t(e: '${1:click}', ${2:val}: ${3:string}): void",
			"}>()"
		],
		"description": "TS 定义 Emits"
	},
	"Vue Define Model": {
		"prefix": "vmodel",
		"body": [
			"const ${1:modelValue} = defineModel<${2:string}>({ required: ${3:true} })"
		],
		"description": "Vue defineModel 宏"
	}
}
